{
  "cli": {
    "version": ">= 12.0.0",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal"
    },
    "preview": {
      "distribution": "internal",
      "android": {
        "buildType": "apk"
      }
      
    },
    "production": {
      "autoIncrement": true,
      "env": {
        "EXPO_PUBLIC_API_BASE_URL": "https://nufogy-api.fly.dev" 
      }
    },
    "test": {
      "env": {
        "EXPO_PUBLIC_API_BASE_URL": "https://nufogy-api.fly.dev" 
      }
    }
  },
  "submit": {
    "production": {}
  }
}
